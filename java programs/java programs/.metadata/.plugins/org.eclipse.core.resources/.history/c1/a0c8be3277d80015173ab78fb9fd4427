package array_prog;

public class Distinct {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		
		int a[]= {2,1,1,2,3,1};
		int un =0;
		
		for(int i=0;i<= (a.length-1);i++)
		{
			for(int j=0;j<=(a.length-1);j++)
			{
				if(a[i]!= a[j+1])
				{
				  un = a[i];
				}
			}
		}
		System.out.println(un);
		

	}

}
